.md-modal.md-effect-1#modal-1
  .md-content
    .main_contact
      .email
        %a{href: "mailto:{{ site.contact_email }}"}
          {{ site.contact_email }}
    .secondary_contact
      %p.address
        {{ site.physical_location }}
      .social
        %a{href: "https://twitter.com/{{ site.twitter_username }}", target: "_blank"}
          %i.fa.fa-twitter
        %a{href: "https://dribbble.com/{{ site.dribbble_username }}", target: "_blank"}
          %i.fa.fa-dribbble
.md-overlay
